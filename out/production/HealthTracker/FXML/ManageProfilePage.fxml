<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox prefHeight="600.0" prefWidth="800.0" styleClass="base" stylesheets="@../style.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.ManageProfilePageController">
   <children>
      <Label text="Manage Profile" textAlignment="CENTER">
         <font>
            <Font size="27.0" />
         </font>
         <VBox.margin>
            <Insets />
         </VBox.margin>
         <padding>
            <Insets left="10.0" top="10.0" />
         </padding>
      </Label>
      <TabPane prefHeight="480.0" prefWidth="700.0" styleClass="tabPane" tabClosingPolicy="UNAVAILABLE" VBox.vgrow="ALWAYS">
        <tabs>
            <Tab styleClass="parentTab" text="Manage Profile">
               <content>
                  <BorderPane prefHeight="200.0" prefWidth="200.0">
                     <center>
                        <VBox prefHeight="200.0" prefWidth="100.0" BorderPane.alignment="CENTER">
                           <children>
                              <HBox alignment="CENTER" prefHeight="53.0" prefWidth="620.0">
                                 <children>
                                    <Label text="Set Sex">
                                       <font>
                                          <Font size="30.0" />
                                       </font>
                                    </Label>
                                 </children>
                                 <VBox.margin>
                                    <Insets top="15.0" />
                                 </VBox.margin>
                              </HBox>
                              <HBox alignment="TOP_CENTER" prefHeight="43.0" prefWidth="620.0">
                                 <children>
                                    <ChoiceBox fx:id="sexCombo" prefHeight="42.0" prefWidth="170.0" />
                                 </children>
                              </HBox>
                              <HBox alignment="CENTER" prefHeight="22.0" prefWidth="620.0">
                                 <children>
                                    <Label fx:id="sexLabel" />
                                 </children>
                              </HBox>
                              <HBox alignment="CENTER" prefHeight="53.0" prefWidth="620.0">
                                 <children>
                                    <Label text="Set Date Of Birth">
                                       <font>
                                          <Font size="30.0" />
                                       </font>
                                    </Label>
                                 </children>
                              </HBox>
                              <HBox alignment="TOP_CENTER" prefHeight="47.0" prefWidth="620.0">
                                 <children>
                                    <DatePicker fx:id="datePick" prefHeight="46.0" prefWidth="193.0" />
                                 </children>
                              </HBox>
                              <HBox alignment="CENTER" prefHeight="22.0" prefWidth="620.0">
                                 <children>
                                    <Label fx:id="dobLabel" />
                                 </children>
                              </HBox>
                              <HBox alignment="CENTER" prefHeight="53.0" prefWidth="620.0">
                                 <children>
                                    <Label text="Set Height">
                                       <font>
                                          <Font size="30.0" />
                                       </font>
                                    </Label>
                                 </children>
                              </HBox>
                              <HBox alignment="TOP_CENTER" prefHeight="47.0" prefWidth="620.0">
                                 <children>
                                    <TextField fx:id="heightField" prefHeight="46.0" prefWidth="166.0" />
                                    <ChoiceBox fx:id="heightUnits" prefHeight="50.0" prefWidth="66.0" />
                                 </children>
                              </HBox>
                              <HBox alignment="CENTER" prefHeight="22.0" prefWidth="620.0">
                                 <children>
                                    <Label fx:id="heightLabel" />
                                 </children>
                              </HBox>
                              <HBox alignment="TOP_CENTER" prefHeight="34.0" prefWidth="620.0">
                                 <children>
                                    <Button fx:id="submitButton" mnemonicParsing="false" onAction="#submitButtonAction" text="Submit" />
                                 </children>
                              </HBox>
                           </children>
                        </VBox>
                     </center>
                  </BorderPane>
               </content>
            </Tab>
          <Tab styleClass="parentTab" text="Calculate Metrics">
               <content>
                  <BorderPane prefHeight="200.0" prefWidth="200.0">
                     <center>
                        <VBox prefHeight="414.0" prefWidth="620.0" BorderPane.alignment="CENTER">
                           <children>
                              <VBox prefHeight="178.0" prefWidth="620.0">
                                 <children>
                                    <HBox alignment="CENTER" prefHeight="36.0" prefWidth="620.0">
                                       <children>
                                          <Label prefHeight="45.0" prefWidth="148.0" text="Calculate BMI">
                                             <font>
                                                <Font size="23.0" />
                                             </font>
                                          </Label>
                                       </children>
                                    </HBox>
                                    <HBox alignment="CENTER" prefHeight="56.0" prefWidth="620.0">
                                       <children>
                                          <VBox alignment="CENTER" prefHeight="50.0" prefWidth="238.0" />
                                          <Button fx:id="showBMIButton" mnemonicParsing="false" onAction="#showBMIButtonAction" prefHeight="49.0" prefWidth="108.0" text="Show BMI" />
                                          <VBox alignment="CENTER" prefHeight="50.0" prefWidth="238.0" />
                                       </children>
                                    </HBox>
                                    <VBox alignment="TOP_CENTER" prefHeight="24.0" prefWidth="620.0">
                                       <children>
                                          <Label fx:id="BMILabel" alignment="CENTER" contentDisplay="CENTER" prefHeight="37.0" prefWidth="118.0" text="??">
                                             <font>
                                                <Font size="30.0" />
                                             </font>
                                          </Label>
                                       </children>
                                    </VBox>
                                    <VBox alignment="TOP_CENTER" prefHeight="38.0" prefWidth="620.0">
                                       <children>
                                          <Label prefHeight="22.0" prefWidth="355.0" text="Values Will Be Calculated Most Recently Input Weight and Height " textAlignment="CENTER" />
                                          <Label text="(Note that BMI is not an accurate representation of health)" />
                                       </children>
                                    </VBox>
                                 </children>
                              </VBox>
                              <VBox prefHeight="16.0" prefWidth="620.0" />
                              <Separator prefWidth="200.0" />
                              <VBox prefHeight="201.0" prefWidth="620.0">
                                 <children>
                                    <HBox alignment="CENTER" prefHeight="36.0" prefWidth="620.0">
                                       <children>
                                          <Label prefHeight="36.0" prefWidth="257.0" text="Calculate Body Fat Index">
                                             <font>
                                                <Font size="23.0" />
                                             </font>
                                          </Label>
                                       </children>
                                    </HBox>
                                    <VBox prefHeight="177.0" prefWidth="620.0">
                                       <children>
                                          <HBox alignment="CENTER_LEFT" prefHeight="58.0" prefWidth="620.0">
                                             <children>
                                                <HBox alignment="CENTER_RIGHT" prefHeight="25.0" prefWidth="185.0">
                                                   <children>
                                                      <Label alignment="CENTER_RIGHT" prefHeight="21.0" prefWidth="110.0" text="Waist (cm): " textAlignment="RIGHT">
                                                         <font>
                                                            <Font size="15.0" />
                                                         </font>
                                                      </Label>
                                                      <TextField fx:id="bodyFatWaistInput" prefHeight="25.0" prefWidth="120.0" />
                                                   </children>
                                                </HBox>
                                                <VBox alignment="CENTER_LEFT" prefHeight="58.0" prefWidth="150.0">
                                                   <children>
                                                      <Label fx:id="bodyFatWaistPopUp" />
                                                   </children></VBox>
                                                <VBox prefHeight="58.0" prefWidth="266.0">
                                                   <children>
                                                      <Label alignment="TOP_LEFT" prefHeight="47.0" prefWidth="265.0" text="Measure the circumference of your waist around the navel for men and around the smallest width for women" wrapText="true">
                                                         <padding>
                                                            <Insets left="5.0" top="5.0" />
                                                         </padding>
                                                         <font>
                                                            <Font size="10.0" />
                                                         </font>
                                                      </Label>
                                                   </children>
                                                </VBox>
                                             </children>
                                          </HBox>
                                          <HBox alignment="CENTER_LEFT" prefHeight="58.0" prefWidth="620.0">
                                             <children>
                                                <HBox alignment="CENTER_RIGHT" prefHeight="25.0" prefWidth="185.0">
                                                   <children>
                                                      <Label alignment="CENTER_RIGHT" prefHeight="21.0" prefWidth="110.0" text="Neck (cm): ">
                                                         <font>
                                                            <Font size="15.0" />
                                                         </font>
                                                      </Label>
                                                      <TextField fx:id="bodyFatNeckInput" prefHeight="25.0" prefWidth="120.0" />
                                                   </children>
                                                </HBox>
                                                <HBox alignment="CENTER_LEFT" prefHeight="49.0" prefWidth="70.0">
                                                   <children>
                                                      <Label fx:id="bodyFatNeckPopUp" />
                                                   </children></HBox>
                                                <VBox alignment="CENTER_LEFT" prefHeight="200.0" prefWidth="80.0">
                                                   <children>
                                                      <Button fx:id="bodyFatSubmit" mnemonicParsing="false" onAction="#bodyFatSubmitAction" text="Calculate" />
                                                   </children>
                                                </VBox>
                                                <VBox prefHeight="58.0" prefWidth="266.0">
                                                   <children>
                                                      <Label alignment="TOP_LEFT" prefHeight="47.0" prefWidth="265.0" text="Measure the circumference of the neck from below the larynx" wrapText="true">
                                                         <padding>
                                                            <Insets left="5.0" top="5.0" />
                                                         </padding>
                                                         <font>
                                                            <Font size="10.0" />
                                                         </font>
                                                      </Label>
                                                   </children></VBox>
                                             </children>
                                          </HBox>
                                          <HBox prefHeight="58.0" prefWidth="620.0">
                                             <children>
                                                <HBox alignment="CENTER_LEFT" prefHeight="50.0" prefWidth="620.0">
                                                   <children>
                                                      <HBox alignment="CENTER_RIGHT" prefHeight="47.0" prefWidth="185.0">
                                                         <children>
                                                            <Label alignment="CENTER_RIGHT" prefHeight="21.0" prefWidth="110.0" text="Hips (cm): ">
                                                               <font>
                                                                  <Font size="15.0" />
                                                               </font>
                                                            </Label>
                                                            <TextField fx:id="bodyFatHipsInput" editable="false" prefHeight="25.0" prefWidth="120.0" />
                                                         </children>
                                                      </HBox>
                                                      <VBox alignment="CENTER_LEFT" prefHeight="200.0" prefWidth="150.0">
                                                         <children>
                                                            <Label fx:id="bodyFatHipsPopUp" />
                                                         </children></VBox>
                                                      <VBox prefHeight="58.0" prefWidth="266.0">
                                                         <children>
                                                            <Label alignment="TOP_LEFT" prefHeight="47.0" prefWidth="265.0" text="(Women only) Measure the circumference of the hips at the largest horizontal measure" wrapText="true">
                                                               <padding>
                                                                  <Insets left="5.0" top="5.0" />
                                                               </padding>
                                                               <font>
                                                                  <Font size="10.0" />
                                                               </font>
                                                            </Label>
                                                         </children></VBox>
                                                   </children>
                                                </HBox>
                                             </children>
                                          </HBox>
                                          <VBox alignment="TOP_CENTER" prefHeight="24.0" prefWidth="620.0">
                                             <children>
                                                <Label fx:id="bodyFatLabel" alignment="CENTER" contentDisplay="CENTER" prefHeight="37.0" prefWidth="118.0" text="??">
                                                   <font>
                                                      <Font size="30.0" />
                                                   </font>
                                                </Label>
                                             </children>
                                          </VBox>
                                       </children>
                                    </VBox>
                                 </children>
                              </VBox>
                           </children>
                        </VBox>
                     </center>
                  </BorderPane>
               </content>
          </Tab>
            <Tab styleClass="parentTab" text="Export Data">
               <content>
                  <BorderPane prefHeight="200.0" prefWidth="200.0">
                     <center>
                        <VBox prefHeight="200.0" prefWidth="100.0" spacing="10.0" BorderPane.alignment="CENTER">
                           <children>
                              <VBox prefHeight="25.0" prefWidth="615.0">
                                 <VBox.margin>
                                    <Insets left="5.0" top="10.0" />
                                 </VBox.margin>
                                 <children>
                                    <Label prefHeight="35.0" prefWidth="158.0" text="Select Information To Export" />
                                 </children>
                              </VBox>
                              <HBox alignment="CENTER" prefHeight="25.0" prefWidth="620.0" spacing="10.0">
                                 <children>
                                    <CheckBox fx:id="nutritionCheckBox" mnemonicParsing="false" selected="true" text="Nutrition data" />
                                    <Separator orientation="VERTICAL" prefHeight="200.0" />
                                    <CheckBox fx:id="exerciseCheckBox" mnemonicParsing="false" selected="true" text="Exercise data" />
                                    <Separator orientation="VERTICAL" prefHeight="200.0" />
                                    <CheckBox fx:id="weightCheckBox" mnemonicParsing="false" selected="true" text="Weight data" />
                                 </children>
                                 <padding>
                                    <Insets left="10.0" />
                                 </padding>
                              </HBox>
                              <HBox alignment="CENTER" layoutX="10.0" layoutY="45.0" prefHeight="25.0" prefWidth="620.0" spacing="10.0">
                                 <padding>
                                    <Insets left="10.0" />
                                 </padding>
                                 <children>
                                    <Button fx:id="exportButton" mnemonicParsing="false" onAction="#exportCSV" text="Export" />
                                 </children>
                              </HBox>
                              <HBox alignment="CENTER" layoutX="10.0" layoutY="90.0" prefHeight="25.0" prefWidth="620.0" spacing="10.0">
                                 <padding>
                                    <Insets left="10.0" />
                                 </padding>
                                 <children>
                                    <Label fx:id="statusLabel" textFill="#68ff54">
                                       <font>
                                          <Font size="18.0" />
                                       </font>
                                    </Label>
                                 </children>
                              </HBox>
                           </children>
                        </VBox>
                     </center>
                  </BorderPane>
               </content>
            </Tab>
        </tabs>
         <VBox.margin>
            <Insets bottom="50.0" left="50.0" right="50.0" />
         </VBox.margin>
         <padding>
            <Insets bottom="25.0" left="40.0" right="40.0" top="25.0" />
         </padding>
      </TabPane>
   </children>
</VBox>
