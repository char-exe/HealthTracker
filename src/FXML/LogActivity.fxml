<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="600.0" prefWidth="800.0" spacing="10.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.LogActivityController">
   <children>
      <Label text="Log activity">
         <VBox.margin>
            <Insets />
         </VBox.margin>
         <padding>
            <Insets left="10.0" top="10.0" />
         </padding>
         <font>
            <Font size="22.0" />
         </font></Label>
      <TabPane prefHeight="200.0" prefWidth="200.0" tabClosingPolicy="UNAVAILABLE" VBox.vgrow="ALWAYS">
        <tabs>
          <Tab closable="false" text="Add exercise">
            <content>
              <AnchorPane />
            </content>
          </Tab>
          <Tab closable="false" text="Update weight">
            <content>
              <AnchorPane />
            </content>
          </Tab>
            <Tab closable="false" text="Add nutrition">
               <content>
                  <AnchorPane />
               </content>
            </Tab>
        </tabs>
      </TabPane>
   </children>
</VBox>
